{
  "expo": {
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "useLegacyPackaging": true
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "production",
          "smallIcons": ["./assets/ic_stat_onesignal_default.png"]
        }
      ],
      "expo-font"
    ],
    "name": "Suki AI",
    "slug": "suki-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#536DFE"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": "1.0.0",
      "bundleIdentifier": "com.sukiai",
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["sms", "tel", "mailto", "http", "https"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#536DFE"
      },
      "package": "com.sukiai",
      "runtimeVersion": "1.0.0",
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "https"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "http"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "mailto"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "tel"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "sms"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.sukiai.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.sukiai.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "7d6950b5-dc68-4ebc-9303-1ba77082c073"
      }
    }
  }
}
